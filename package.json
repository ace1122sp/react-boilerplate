    "moduleNameMapper": {
      "^.+\\.(css|less|scss)$": "babel-jest"
    },
    "verbose": true
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ace1122sp/react-boilerplate.git"
  },
  "author": "ace11",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ace1122sp/react-boilerplate/issues"
  },
  "homepage": "https://github.com/ace1122sp/react-boilerplate#readme",
  "dependencies": {
    "autoprefixer": "^9.8.6",
    "bootstrap": "^4.5.2",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.4",
    "jquery": "^3.5.1",
    "popper.js": "^1.16.1",
    "prop-types": "^15.7.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-router-dom": "^5.2.0",
    "regenerator-runtime": "^0.13.7"
