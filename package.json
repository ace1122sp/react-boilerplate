  "version": "1.0.0",
  "description": "simple react setup",
  "main": "index.js",
  "scripts": {
    "start": "webpack-dev-server --hot --mode development",
    "dev": "webpack --mode development --watch",
    "build": "webpack --mode production",
    "lint": "eslint ./client",
    "lint:fix": "eslint --fix ./client",
    "test": "jest"
  },
  "jest": {
    "setupFilesAfterEnv": [
      "./client/setupTests.js"
